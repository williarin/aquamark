{
    "require": {
        "symfony/dependency-injection": "^7.0",
        "symfony/config": "^7.0",
        "symfony/options-resolver": "^7.0",
        "imagine/imagine": "^1.3",
        "automattic/jetpack-autoloader": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Williarin\\FreeWatermarks\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11",
        "10up/wp_mock": "^0.4.2"
    },
    "autoload-dev": {
        "psr-4": {
            "Williarin\\FreeWatermarks\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "allow-plugins": {
            "automattic/jetpack-autoloader": true
        }
    }
}